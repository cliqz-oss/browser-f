<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Start Page</ShortName> 
  <Description>Search StartPage</Description> 
  <InputEncoding>UTF-8</InputEncoding> 
  <Image height="16" width="16" type="image/x-icon">data:image/png;base64,</Image>
  <Url type="text/html" method="GET" template="https://www.startpage.com/do/dsearch" rel="searchform">
    <Param name="query" value="{searchTerms}"/>
    <Param name="cat" value="web"/>
    <Param name="pl" value="opensearch"/>
    <Param name="language" value="deutsch"/>
  </Url>
</SearchPlugin>
